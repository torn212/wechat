.center {
	height: calc(100% - 2.2rem);
	width: 100%;
	overflow: auto;
	position: fixed;
}

.centerpadding {
	padding-left: 0.5rem;
	padding-right: 0.5rem;
	padding-top: 0rem;
	padding-bottom: 1rem;
}

.header {
	width: 100%;
	height: 2rem;
	background: #8b1d3a;
	text-align: center;
	color: #fff;
	font-size: 1rem;
	line-height: 2rem;
	font-weight: normal;
	font-family: simsun;
	position: relative;
}

.colorred {
	color: red;
}

.wordbreak {
	word-break: break-all;
}

.uploadloading {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}

.closeimg {
	position: absolute;
	top: 0px;
	right: 5px;
	transform: translate(0, 0);
}

.weui_uploader_status_me {
	position: relative;
}

.weui_uploader_status_me:before {
	content: " ";
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
}
